    ViewBag.Title = "AddAbout";
    Layout = "~/Views/Shared/_AdminLayoutPage.cshtml";
}
<br />
<h3>Hakkımızda Ekle</h3>
<br />
<form method="post" enctype="multipart/form-data">
    <label>Başlık Ekle</label>
